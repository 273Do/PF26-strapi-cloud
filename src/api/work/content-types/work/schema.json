{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Work"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "releaseDate": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "article": {
      "type": "richtext",
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "technologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology"
    },
    "githubLink": {
      "type": "string"
    },
    "appLink": {
      "type": "string"
    },
    "otherLink": {
      "type": "string"
    },
    "uuid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid",
      "options": {
        "disable-auto-fill": false
      }
    }
  }
}
